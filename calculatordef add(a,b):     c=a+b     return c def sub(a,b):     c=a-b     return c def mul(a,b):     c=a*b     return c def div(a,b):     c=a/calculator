def add(a,b):
    c=a+b
    return c
def sub(a,b):
    c=a-b
    return c
def mul(a,b):
    c=a*b
    return c
def div(a,b):
    c=a/b
    return c
a=int(input("A:"))
b=int(input("B:"))
x=int(input("1.add 2.sub 3.mul 4. div"))
if x==1:
    ans= add(a,b)
    print(ans)
elif x==2:
    ans= sub(a,b)
    print(ans)
elif x==3:
    ans= mul(a,b)
    print(ans)
elif x==4:
    ans= div(a,b)
    print(ans)
